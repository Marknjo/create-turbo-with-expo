{
  "extends": "@mj/tsconfig/nextjs.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "rootDir": ".",
    "allowJs": true
    // "paths": {
    //   "@/*": ["./*"]
    // }
  },
  "types": ["nativewind/types"],
  "include": [
    "./**/*.ts",
